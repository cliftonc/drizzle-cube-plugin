{
  "name": "drizzle-cube-plugin",
  "version": "1.0.0",
  "description": "Claude Code plugin for Drizzle Cube - MCP tools for semantic layer analytics",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "drizzle-cube-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node18 --format=esm --outfile=dist/index.js --banner:js='#!/usr/bin/env node'",
    "build:types": "tsc --emitDeclarationOnly",
    "start": "node dist/index.js",
    "dev": "tsc --watch"
  },
  "keywords": [
    "mcp",
    "claude-code",
    "drizzle-cube",
    "semantic-layer",
    "analytics",
    "claude-plugin"
  ],
  "author": "Clifton Cunningham <clifton.cunningham@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cliftonc/drizzle-cube-plugin.git"
  },
  "homepage": "https://try.drizzle-cube.dev",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.0.0"
  }
}
